{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.cjs"
    },
    {
      "src": "/admin",
      "dest": "server.cjs"
    },
    {
      "src": "/admin/(.*)",
      "dest": "server.cjs"
    },
    {
      "src": "/exhibitions",
      "dest": "/dist/index.html"
    },
    {
      "src": "/artists",
      "dest": "/dist/index.html"
    },
    {
      "src": "/collection",
      "dest": "/dist/index.html"
    },
    {
      "src": "/about",
      "dest": "/dist/index.html"
    },
    {
      "src": "/contact",
      "dest": "/dist/index.html"
    },
    {
      "src": "/exhibition/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/artist/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/artwork/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE_URL": "postgresql://neondb_owner:npg_dinsL9yt8SbF@ep-empty-wave-agkozpl3-pooler.c-2.eu-central-1.aws.neon.tech/neondb?sslmode=require",
    "POSTGRES_URL": "postgresql://neondb_owner:npg_dinsL9yt8SbF@ep-empty-wave-agkozpl3-pooler.c-2.eu-central-1.aws.neon.tech/neondb?sslmode=require",
    "JWT_SECRET": "aether-art-space-jwt-secret-2024"
  }
}
